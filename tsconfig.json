{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "es6"
  },
  "allowJs": true,
  "esModuleInterop": true,
  "jsx": "react-native",
  "lib": ["DOM", "ESNext"],
  "modeResolution": "node",
  "noEmit": true,
  "resolveJsonModule": true,
  "skipLibCheck": true,
  "target": "ESNext"
}
